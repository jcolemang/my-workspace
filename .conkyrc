
conky.config = {
  alignment = 'top_left',
  background = false,
  update_interval = 5.0
}

conky.text = [[
  ${time %c}

  ${texeci 144000 get-pull-requests ~/.github-repos.yaml}

  Battery --> $battery_percent%

  ${color grey}RAM --> $color $mem/$memmax - $memperc% ${membar 4}

  Usage:$color $cpu%

  ]]
